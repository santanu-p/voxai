GEMINI_API_KEY=your_gemini_api_key

# Required in production for /api/token origin validation (comma-separated exact origins).
# Example: https://voxai.example.com,https://www.voxai.example.com
# No trailing slashes.
VOXAI_ALLOWED_ORIGINS=

# Optional server config
VOXAI_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
VOXAI_DEFAULT_VOICE=Aoede
VOXAI_TOKEN_RATE_WINDOW_MS=60000
VOXAI_TOKEN_RATE_LIMIT_PER_IP=12
VOXAI_TOKEN_RATE_LIMIT_GLOBAL=300
VOXAI_TOKEN_MAX_INFLIGHT=120
