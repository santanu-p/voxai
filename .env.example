GEMINI_API_KEY=your_gemini_api_key

# Required in production for /api/token origin validation (comma-separated exact origins).
# Example: https://noa-live.example.com,https://www.noa-live.example.com
# No trailing slashes.
NOA_ALLOWED_ORIGINS=

# Optional server config
NOA_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
NOA_DEFAULT_VOICE=Aoede
NOA_TOKEN_RATE_WINDOW_MS=60000
NOA_TOKEN_RATE_LIMIT_PER_IP=12
NOA_TOKEN_RATE_LIMIT_GLOBAL=300
NOA_TOKEN_MAX_INFLIGHT=120
