GEMINI_API_KEY=your_gemini_api_key

# Required in production (comma-separated exact origins).
# Example: https://voxai.example.com,https://www.voxai.example.com
VOXAI_ALLOWED_ORIGINS=

# Optional server config
HOST=0.0.0.0
PORT=3000
VOXAI_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
VOXAI_DEFAULT_VOICE=Aoede
VOXAI_MAX_PAYLOAD_BYTES=4194304
VOXAI_MAX_CONNECTIONS=200
VOXAI_MAX_CONNECTIONS_PER_IP=10
VOXAI_MAX_MESSAGES_PER_MINUTE=1500
VOXAI_PING_INTERVAL_MS=30000
VOXAI_START_TIMEOUT_MS=15000
